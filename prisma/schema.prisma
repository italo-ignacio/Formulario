// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  referentialIntegrity = "prisma"
}

model data{
  id Int @id @default(autoincrement())
  nome String? @db.Text()  
  data_nascimento String? @db.Text()
  cpf String? @db.Text()
  endereco String? @db.Text()
  telefone String? @db.Text()
  email String? @db.Text()  
  estado_civil String? @db.Text()  
  nome_antigo String? @db.Text() 
  nacionalidades String? @db.Text() 
  serviu_exercito String? @db.Text() 
  estado_deseja  String? @db.Text() 
  data_deseja  String? @db.Text() 
  tempo_deseja  String? @db.Text() 
  hotel  String? @db.Text() 
  viajar_junto String? @db.Text()
  pagar_viagem String? @db.Text()
  possui_visto String? @db.Text()
  foi_para_eua String? @db.Text()
  visto_per_o_rou String? @db.Text()
  visto_recusado String? @db.Text()
  passaporte_per_o_rou String? @db.Text()
  passaporte_recusado String? @db.Text()
  parentes_nos_eua String? @db.Text()
  nome_pai String? @db.Text()
  data_nasc_pai String? @db.Text()
  pai_mora_eua String? @db.Text()
  nome_mae String? @db.Text()
  data_nasc_mae String? @db.Text()
  mae_mora_eua String? @db.Text()
  instagram String? @db.Text()
  facebook String? @db.Text()
  linkedin String? @db.Text()
  trabalho String? @db.Text()
  nome_empresa String? @db.Text()
  endereco_empresa String? @db.Text()
  telefone_empresa String? @db.Text()
  data_inicio_empresa String? @db.Text()
  trabalho_antigo String? @db.Text()
  nome_empresa_antigo String? @db.Text()
  endereco_empresa_antigo String? @db.Text()
  telefone_empresa_antigo String? @db.Text()
  data_ini_ter_empresa_antigo String? @db.Text()
  facul_escola String? @db.Text()
  nome_facul_escola String? @db.Text()
  endereco_facul_escola String? @db.Text()
  telefone_facul_escola String? @db.Text()
  curso_facul String? @db.Text()
  ini_ter_facul_escola String? @db.Text()
} 